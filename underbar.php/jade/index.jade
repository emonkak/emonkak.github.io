          dd: a(href="#countBy") countBy
          dd: a(href="#shuffle") shuffle
          dd: a(href="#toArray") toArray
          dd: a(href="#size") size
          dd: a(href="#memorize") memorize

          dt.title: a(href="#arrays")
            h2 Array Functions
