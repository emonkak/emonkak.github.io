doctype 5

html(lang="ja")
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no")
    title Underbar.php - an array and lazy list utility, like Underscore.js
    link(href="css/styles.css", rel="stylesheet")
  body
    header#header
      h1.title
        a(href="#")
          h2 Underbar.php
          small Version 1.0.0
        a.button-menu(href="#header") &#x2630;

      nav(role="navigation")
        dl
          dd: a(href="#introduction") Introduction
          dd: a(href="#getting-started") Getting Started

          dt.title: a(href="#Underbar")
            h2 Main Classes
            small Underbar namespace
          dd: a(href="#Strict") Strict
          dd: a(href="#Lazy") Lazy
          dd: a(href="#LazyIterator") LazyIterator
          dd: a(href="#LazyGenerator") LazyGenerator

          dt.title: a(href="#Underbar-Internal")
            h2 Internal Classes
            small Underbar\Internal namespace
          dd: a(href="#Strict") Strict
          dd: a(href="#Lazy") Lazy
          dd: a(href="#LazyIterator") LazyIterator
          dd: a(href="#LazyGenerator") LazyGenerator


          dt.title: a(href="#collections")
            h2 Collection Functions
            small Array or Traversable object
          dd: a(href="#each") each
          dd: a(href="#map") map
